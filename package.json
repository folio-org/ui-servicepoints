{
  "name": "@folio/servicepoints",
  "version": "6.2.0",
  "description": "Service Points handler for Stripes",
  "repository": "folio-org/ui-servicepoints",
  "publishConfig": {
    "registry": "https://repository.folio.org/repository/npm-folio/"
  },
  "license": "Apache-2.0",
  "engines": {
    "node": ">=14.0.0"
  },
  "main": "src/index.js",
  "stripes": {
    "actsAs": [
      "handler"
    ],
    "handlerName": "eventHandler",
    "displayName": "ui-servicepoints.meta.title",
    "route": "/",
    "links": {
      "userDropdown": [
        {
          "event": "CHANGE_SERVICE_POINT",
          "caption": "ui-servicepoints.userDropdown.switchServicePoint",
          "check": "checkServicePoints"
        }
      ]
    }
  },
  "scripts": {
    "start": "stripes serve",
    "lint": "eslint .",
    "test": "jest --ci --coverage",
    "build-mod-descriptor": "stripes mod descriptor --full --strict | jq '.[]' > module-descriptor.json ",
    "formatjs-compile": "formatjs compile-folder --ast --format simple ./translations/ui-servicepoints ./translations/ui-servicepoints/compiled"
  },
  "devDependencies": {
    "@babel/core": "^7.19.0",
    "@folio/eslint-config-stripes": "^5.1.0",
    "@folio/stripes": "^7.0.0",
    "@folio/stripes-cli": "^2.0.0",
    "@folio/stripes-components": "^10.0.0",
    "@folio/stripes-connect": "^7.0.0",
    "@folio/stripes-core": "^8.0.0",
    "@formatjs/cli": "^4.2.20",
    "@jest/globals": "^26",
    "@testing-library/dom": "^8.17.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12",
    "@testing-library/user-event": "^14.4.3",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^26",
    "babel-plugin-module-resolver": "^4.1.0",
    "chai": "^4.2.0",
    "core-js": "^3.6.4",
    "eslint": "^6.2.1",
    "jest": "^26",
    "jest-canvas-mock": "^2.4.0",
    "jest-junit": "^14.0.1",
    "miragejs": "^0.1.32",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-intl": "^5.7.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "regenerator-runtime": "^0.13.3"
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "prop-types": "^15.6.0"
  },
  "peerDependencies": {
    "@folio/stripes": "^7.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-intl": "^5.7.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0"
  }
}
